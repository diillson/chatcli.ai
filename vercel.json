{
  "routes": [
    {
      "src": "^/$",
      "has": [{ "type": "host", "value": "chatcli.edilsonfreitas.com" }],
      "dest": "/trust"
    },
    {
      "src": "^/(.+)$",
      "has": [{ "type": "host", "value": "chatcli.edilsonfreitas.com" }],
      "dest": "/trust/$1"
    },
    {
      "src": "^/(.*)$",
      "has": [{ "type": "host", "value": "chatcli.edilsonfreitas.com" }],
      "status": 308,
      "headers": { "Location": "https://chatcli.edilsonfreitas.com/$1" }
    },
    {
      "src": "^/(.*)$",
      "has": [{ "type": "host", "value": "www.chatcli.edilsonfreitas.com" }],
      "status": 308,
      "headers": { "Location": "https://chatcli.edilsonfreitas.com/$1" }
    }
  ]
}
